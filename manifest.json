{
    "manifest_version": 2,
    "name": "kalevis_",
    "author": "aimeric",
    "description": "notifie quand kalevis_ est en live",
    "version": "1.1.0",
    "permissions": [
        "http://*/*",
        "https://*/*",
        "notifications",
        "storage"

    ],
    "browser_action": {
        "default_popup": "index.html"
        
    },
    "icons" : {
        "70" : "img/icon_offline.png"
    },
    
    "background": {
        "persistent": false,
        "scripts": [
            "js/jquery.js",
            "js/background.js"
            
        ]
    }


}